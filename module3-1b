a=0
b=1
sum=0
count=1
while(count<=n):
    print(sum)
    count+=1
    a=b
    b=sum
    sum=a+b
    
    
    
    word = input("Input a word to reverse: ")

for char in range(len(word) - 1, -1, -1):
  print(word[char], end="")
